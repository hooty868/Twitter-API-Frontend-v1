      <div class=\"setting-pannel col-3 h-100\">\n        <Navbar @after-create-twitter=\"afterCreateTwitter\" />\n      </div>\n      <div class=\"main-content col h-100\">\n        <div class=\"header\"><p>首頁</p></div>\n        <div class=\"twitter-bar d-flex\">\n          <img class=\"profile-avater\" :src=\"currentUser.avatar\" alt=\"avater\" />\n          <form class=\"form-group\" @submit.stop.prevent=\"handleSubmit\">\n            <input\n              id=\"twitter\"\n              v-model=\"twitter\"\n              name=\"twitter\"\n              type=\"twitter\"\n              class=\"twitter-input form-control\"\n              autocomplete=\"twitter\"\n              placeholder=\"有什麼新鮮事？\"\n              required\n              autofocus\n            />\n            <button class=\"btn btn-lg btn-submit btn-block mb-3\" type=\"submit\">\n              推文\n            </button>\n          </form>\n        </div>\n        <twitterList :replied-twitter=\"repliedTwitter\" :Twitters=\"twitters\" />\n      </div>\n      <div class=\"main-follower col-4 h-100\">\n        <Followers :user-profile=\"currentUser\" :follower-list=\"followerlist\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Navbar from \"./../components/Navbar\";\nimport Followers from \"./../components/followers\";\nimport twitterList from \"./../components/twitterList\";\nimport twitterAPI from \"./../apis/twitter\";\n// import { Toast } from \"./../utils/helpers\";\n\n// const dummyUser = {\n//   currentUser: {\n//     id: 61,\n//     account: \"hooty\",\n//     name: \"hooty\",\n//     email: \"hooty@gmail.com\",\n//     avatar: \"https://loremflickr.com/320/240/corgi?lock=22\",\n//     cover: null,\n//     introduction: null,\n//     followerCount: 0,\n//     followingCount: 0,\n//     isFollowed: 0,\n//     isCurrentUser: true,\n//   },\n// };\n// const dummyData = {\n//   followerlist: [\n//     {\n//       followingId: 11,\n//       name: \"user1\",\n//       account: \"user1\",\n//       avatar: \"https://loremflickr.com/320/240/corgi?lock=22\",\n//       isFollowed: 0,\n//     },\n//     {\n//       followingId: 21,\n//       name: \"user2\",\n//       account: \"user2\",\n//       avatar: \"https://loremflickr.com/320/240/corgi?lock=46.44188312924983\",\n//       isFollowed: 0,\n//     },\n//     {\n//       followingId: 31,\n//       name: \"user3\",\n//       account: \"user3\",\n//       avatar: \"https://loremflickr.com/320/240/corgi?lock=26.41331684533894\",\n//       isFollowed: 0,\n//     },\n//     {\n//       followingId: 41,\n//       name: \"user4\",\n//       account: \"user4\",\n//       avatar: \"https://loremflickr.com/320/240/corgi?lock=35.78072479906069\",\n//       isFollowed: 0,\n//     },\n//   ],\n// };\nexport default {\n  components: {\n    Navbar,\n    Followers,\n    twitterList,\n  },\n  created() {\n    this.fetchList();\n  },\n  methods: {\n    async fetchList() {\n      try {\n        const response = await twitterAPI.TwitterAll();\n        this.twitters = response.data;\n      } catch (error) {\n        console.log(\"error\", error);\n      }\n    },\n    afterCreateTwitter(payload) {\n      const { Description } = payload;\n      this.repliedTwitter = {\n        id: Math.floor(Math.random() * 100) + 1,\n        User: {\n          id: this.currentUser.id,\n          account: this.currentUser.account,\n          name: this.currentUser.name,\n          email: this.currentUser.email,\n          avatar: this.currentUser.avatar,\n          cover: this.currentUser.cover,\n          isFollowed: this.currentUser.isFollowed,\n          introduction: this.currentUser.introduction,\n          followerCount: this.currentUser.followerCount,\n          followingCount: this.currentUser.followingCount,\n        },\n        repliedCount: Math.floor(Math.random() * 100) + 1,\n        followedCount: Math.floor(Math.random() * 100) + 1,\n        createdAt: new Date(),\n        updatedAt: \"\",\n        isliked: true,\n        description: Description,\n      };\n    },\n    handleSubmit() {\n      this.repliedTwitter = {\n        id: Math.floor(Math.random() * 100) + 1,\n        User: {\n          id: this.currentUser.id,\n          account: this.currentUser.account,\n          name: this.currentUser.name,\n          email: this.currentUser.email,\n          avatar: this.currentUser.avatar,\n          cover: this.currentUser.cover,\n          isFollowed: this.currentUser.isFollowed,\n          introduction: this.currentUser.introduction,\n          followerCount: this.currentUser.followerCount,\n          followingCount: this.currentUser.followingCount,\n        },\n        repliedCount: Math.floor(Math.random() * 100) + 1,\n        followedCount: Math.floor(Math.random() * 100) + 1,\n        createdAt: new Date(),\n        updatedAt: \"\",\n        isliked: true,\n        description: this.twitter,\n      };\n      this.twitter = \"\";\n    },\n  },\n  data() {\n    return {\n      twitter: \"\",\n      currentUser: {\n        id: -1,\n        account: \"\",\n        name: \"\",\n        email: \"\",\n        avatar: \"\",\n        cover: \"\",\n        followerCount: -1,\n        followingCount: 0,\n        isFollowed: false,\n        introduction: \"\",\n      },\n      repliedTwitter: {\n        id: -1,\n        User: {\n          id: 14,\n          account: \"bmw\",\n          name: \"BMW\",\n          email: \"BMW@example.com\",\n          avatar: \"https://i.pravatar.cc/300\",\n          cover: \"https://i.pravatar.cc/300\",\n          isFollowed: true,\n          introduction: \"hello world!\",\n          followerCount: 1,\n          followingCount: 3,\n        },\n        repliedCount: -1,\n        followedCount: -1,\n        createdAt: \"\",\n        updatedAt: \"\",\n        isliked: true,\n        description: \"\",\n      },\n      twitters: [],\n      followerlist: [],\n    };\n  },\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 1440px;\n  height: