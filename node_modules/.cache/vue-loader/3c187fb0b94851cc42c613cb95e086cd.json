         <li>\n              <strong>{{ FavoritedRestaurants.length }}</strong>\n              收藏的餐廳\n            </li>\n            <li>\n              <strong>{{ Followers.length }}</strong> followings (追蹤者)\n            </li>\n            <li>\n              <strong>{{ Followings.length }}</strong> followers (追隨者)\n            </li>\n          </ul>\n          <p>\n            <a href=\"/users/1/edit\"\n              ><button\n                type=\"submit\"\n                class=\"btn btn-primary\"\n                v-if=\"initialisfollowed\"\n              >\n                edit\n              </button></a\n            >\n          </p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    initialUser: {\n      type: Object,\n      required: true,\n    },\n    initialComments: {\n      type: Array,\n      required: true,\n    },\n    initialFavoritedrestaurants: {\n      type: Array,\n      required: true,\n    },\n    initialFollowers: {\n      type: Array,\n      required: true,\n    },\n    initialFollowings: {\n      type: Array,\n      required: true,\n    },\n    initialIsfollowed: {\n      type: Boolean,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      User: this.initialUser,\n      Comments: this.initialComments,\n      FavoritedRestaurants: this.initialFavoritedrestaurants,\n      Followers: this.initialFollowers,\n      Followings: this.initialFollowings,\n      initialisfollowed: this.initialIsfollowed,\n    };\n  },\n  methods: {\n    addFollowing() {\n      this.User = {\n        ...this.Followings, // 保留餐廳內原有資料\n        isFavorited: true,\n      };\n    },\n    deleteFollowing() {\n      this.restaurant = {\n        ...this.Followings, // 保留餐廳內原有資料\n        isFavorited: false,\n      };\n    },\n  },\n};\n</script>"]}]}